#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eVungle extension on android
platform JAVA

files
{
    (source/android)
    s3eVungle.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "vunglepub/VunglePub.jar"

option output-name=lib/android/s3eVungle.jar
