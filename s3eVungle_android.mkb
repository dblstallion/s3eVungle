#!/usr/bin/env mkb
# Builder mkb file for the s3eVungle extension on android
platform LINUX

buildtype (SCONS, ARM)

define S3E_ANDROID

options
{
    dll
    output-name="lib/android/libs3eVungle.so"
    output-name-debug="lib/android/libs3eVungle_d.so"
}
define S3E_BUILD_EXT

subproject s3eVungle_build
